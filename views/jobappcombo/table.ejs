<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <style>
        * {
            margin: 0;
            padding: 0;
            box-sizing: border-box;
            font-size: 14px;
        }

        .container {
            padding: 60px;
            width: 100%;
            display: flex;
            flex-direction: column;
            justify-content: center;
            align-items: center;
            gap: 50px;
        }

        table {
            border-spacing: 0;
        }

        tr.header td {
            font-weight: bold;
            color: green;
            font-size: 15px;

        }

        tr td {
            padding: 6px;
        }

        table tr td {
            text-align: center;
        }
    </style>
</head>
<body>
    <div class="container">
        <table border="1">
            <tr>
                <td>Id</td>
                <td>Name</td>
                <td>email</td>
                <td>Edit</td>
            </tr>
            <% data.forEach((user) => { %>
                <tr>
                    <td><%= user.canid %></td>
                    <td><%= user.fname + " "+ user.lname %></td>
                    <td><%= user.email %></td>
                    <td><a href="/jobapp/edit/<%= user.canid %>" id="user<%= user.canid %>" class="edit">Edit</a></td>
                </tr>
            <% }) %>
        </table>
    </div>
</body>
</html>